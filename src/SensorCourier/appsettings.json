{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Provider": "MySql",
  "ConnectionStrings": {
    "MySql": "Server=<HOST>;Port=<PORT>;Database=<DATABASE_NAME>;User=<USERNAME>;Password=<PASSWORD>;",
    "SqlServer": "Server=<SERVER_NAME>;Database=<DATABASE_NAME>;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "MongoDbSettings": {
    "DatabaseName": "<DATABASE_NAME>",
    "ConnectionString": "mongodb://<USERNAME>:<PASSWORD>@<HOST>:<PORT>/?directConnection=true"
  },
  "AppSettings": {
    "BatchDelaySeconds": 100,
    "BatchSize": 100,
    "LastDateTime": "",
    "Monitor": false
  }
}
